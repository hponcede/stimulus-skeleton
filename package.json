{
  "name": "stimulus-skeleton",
  "version": "0.1.0",
  "private": false,
  "scripts": {
    "build": "dotenv cross-var -- parcel build src/index.html --public-url %PUBLIC_PATH%  --experimental-scope-hoisting",
    "dev": "parcel src/index.html --log-level 4",
    "start": "npm run dev",
    "deploy": "dotenv cross-var -- scp -r dist/* %DEPLOY_PATH%"
  },
  "dependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-es2015": "^7.0.0-beta.53",
    "@babel/preset-stage-0": "^7.8.3",
    "core-js": "^3.6.4",
    "dragonbinder": "^2.2.0",
    "stimulus": "^1.1.1",
    "tailwindcss": "^1.4.6"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "cross-var": "^1.1.0",
    "dotenv-cli": "^3.1.0",
    "jsx-no-react": "^0.3.0",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-ascii-header": "^0.3.0",
    "parcel-plugin-clean-dist": "0.0.6",
    "parcel-plugin-metaimage": "^1.2.3",
    "postcss-import": "^12.0.1",
    "postcss-modules": "^2.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0"
  }
}
